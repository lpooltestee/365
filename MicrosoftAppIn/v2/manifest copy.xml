<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MessageCompose">
  <Id>f84b674a-3f12-4a5e-b418-9c290abcd001</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>HoffmannDH</ProviderName>
  <DefaultLocale>pt-BR</DefaultLocale>
  <DisplayName DefaultValue="Assinatura Corporativa">
    <Override Locale="en-US" Value="Corporate Signature" />
  </DisplayName>
  <Description DefaultValue="Insere automaticamente assinatura personalizada ao compor e-mails.">
    <Override Locale="en-US" Value="Automatically inserts a personalized corporate signature." />
  </Description>
  <IconUrl DefaultValue="https://assinaturas.hoffmanndh.com/static/logo.png"/>
  <SupportUrl DefaultValue="https://assinaturas.hoffmanndh.com"/>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.5">
      <Set Name="Mailbox"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://assinaturas.hoffmanndh.com/addin"/>
        <RequestedHeight>200</RequestedHeight>
      </DesktopSettings>
      <WebSettings>
        <SourceLocation DefaultValue="https://assinaturas.hoffmanndh.com/addin"/>
        <RequestedHeight>200</RequestedHeight>
      </WebSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
</OfficeApp>